
% Define Motor Parameters
J = 0.01; b =0.05; Kt = 0.07; Ke = 0.04; R = 0.07; L = 0.09;

a_0 = Kt;
b_0 = J * L; b_1 = J * R + b * L; b_2 = b * R + Kt * Ke;
s = tf("s")
Gp = Kt * s / (J * L * s^2 + (J*R + b*L)*s + (b*R + Kt*Ke))
Gp = tf([a_0],  [b_0, b_1, b_2])
figure();
[x,t] = step(Gp)
plot(t,x, 'LineWidth',3, 'color', 'k'); xlabel('Time [s]'); ylabel('Amplitude'); grid on; hold on;
legend('Speed')
hold off;